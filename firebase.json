{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "private/**",
      "pages/**",
      "**/node_modules/**"
    ]
  }
}
