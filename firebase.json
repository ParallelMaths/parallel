{
  "hosting": {
    "public": "static",
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "private/**",
      "pages/**",
      "**/node_modules/**"
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ]
}
